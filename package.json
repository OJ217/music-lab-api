{
  "name": "music-lab-api",
  "version": "1.0.0",
  "description": "API service for music lab app.",
  "keywords": [
    "aws-lambda",
    "typescript",
    "serverless",
    "hono"
  ],
  "author": "Ochirjav Jargalsaikhan",
  "license": "MIT",
  "scripts": {
    "dev": "serverless offline start --verbose --httpPort 5000",
    "deploy-dev": "serverless deploy --stage dev",
    "deploy-prod": "serverless deploy --stage prod",
    "generate-local-cert": "chmod +x ./generate-cert.sh && ./generate-cert.sh --comment music-lab-office-api --stage local",
    "generate-dev-cert": "chmod +x ./generate-cert.sh && ./generate-cert.sh --comment music-lab-office-api --stage dev",
    "generate-prod-cert": "chmod +x ./generate-cert.sh && ./generate-cert.sh --comment music-lab-office-api --stage prod"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/jsonwebtoken": "^9.0.4",
    "esbuild": "^0.17.19",
    "serverless": "^3.35.2",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.48.5",
    "serverless-offline": "^13.2.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.10",
    "google-auth-library": "^9.1.0",
    "hono": "^3.12.6",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.5.0",
    "papr": "^15.2.1",
    "zod": "^3.22.4"
  }
}
